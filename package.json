{
  "name": "@jupiterone/graph-tenable-cloud",
  "version": "5.0.6",
  "description": "A JupiterOne managed integration for https://www.tenable.com/products/tenable-io",
  "main": "dist/index.js",
  "repository": "https://github.com/JupiterOne/graph-tenable-cloud",
  "license": "SEE LICENSE IN LICENSE",
  "files": [
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "ts-node --require dotenv/config tools/execute.ts | bunyan",
    "start:containers": "docker-compose up -d",
    "stop:containers": "docker-compose down",
    "restart:containers": "docker-compose restart",
    "prebuild": "yarn test",
    "build": "rm -rf dist/* && yarn build:types && yarn build:src && yarn build:docs",
    "build:src": "babel src --extensions '.ts' --out-dir dist",
    "build:docs": "ts-node tools/docs.ts",
    "build:types": "tsc --project tsconfig.dist.json --declaration --emitDeclarationOnly",
    "lint": "yarn lint:ts && yarn lint:md",
    "lint:md": "remark .",
    "lint:ts": "tslint --project .",
    "format": "yarn prettier --write '**/*.{ts,js,json,md}'",
    "test": "jest",
    "prepack": "yarn build"
  },
  "dependencies": {
    "@jupiterone/gremlin-client": "^8.0.0",
    "@jupiterone/jupiter-managed-integration-sdk": "^36.0.1",
    "@jupiterone/jupiter-types": "^44.21.2",
    "@jupiterone/lambda-runtime-tools": "^7.0.0",
    "@lifeomic/attempt": "^3.0.0",
    "node-fetch": "^2.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/bunyan": "^1.8.5",
    "@types/fs-extra": "^8.0.1",
    "@types/gremlin": "^3.4.2",
    "@types/jest": "^24.0.0",
    "@types/nock": "10.0.3",
    "@types/node": "~8.10.0",
    "@types/node-fetch": "^2.3.2",
    "aws-sdk": "^2.828.0",
    "bunyan": "^1.8.12",
    "dotenv": "^8.0.0",
    "fs-extra": "^8.0.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jest-haste-map": "^24.9.0",
    "jest-resolve": "^24.9.0",
    "lint-staged": "^9.4.3",
    "nock": "^10.0.6",
    "prettier": "1.19.1",
    "remark-cli": "^7.0.1",
    "remark-lint": "^6.0.4",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  }
}
